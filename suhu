import java.util.*;

public class suhu
{
	public static void main (String argc [])
	{
		Scanner sc = new Scanner (System.in);
		System.out.println("memasukkan temperatur air (celcius): ");
		int tmpair = sc.nextInt();
		String bentukair = "";
		if ((tmpair>=50) && (tmpair<0))
		bentukair = "padat";
			else if ((tmpair>=0) && (tmpair<100))
			bentukair = "cair";
			else
			bentukair = "gas";
		System.out.println("air dalam suhu" +" " + tmpair + "derajat celcius berbentuk" +" " +bentukair);	
	}
}
